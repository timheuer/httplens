{
  "name": "httplens",
  "displayName": "HTTPLens",
  "description": "A code lens feature to enable adding calls to .http files.",
  "version": "0.1.0",
  "publisher": "timheuer",
  "author": {
    "name": "Tim Heuer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timheuer/httplens.git"
  },
  "qna": "https://github.com/timheuer/httplens/issues",
  "homepage": "https://github.com/timheuer/httplens#readme",
  "icon": "resources/icon.png",
  "license":"MIT",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other",
    "Testing"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "deploy": "vsce publish",
    "package": "npm run compile && vsce package",
    "format": "eslint --fix --ext .ts src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "nerdbank-gitversioning": "^3.8.38-alpha"
  }
}
